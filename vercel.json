{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "installCommand": "cd client && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/server/index.js"
    },
    {
      "source": "/uploads/:path*",
      "destination": "/server/index.js"
    },
    {
      "source": "/:path*",
      "destination": "/client/dist/index.html"
    }
  ]
}
