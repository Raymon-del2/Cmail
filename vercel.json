{
  "version": 2,
  "outputDirectory": "client/dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/server/index.js"
    },
    {
      "source": "/uploads/:path*",
      "destination": "/server/index.js"
    },
    {
      "source": "/:path*",
      "destination": "/client/dist/index.html"
    }
  ]
}
