{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "outputDirectory": "client/dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/server/index.js"
    },
    {
      "source": "/uploads/:path*",
      "destination": "/server/index.js"
    },
    {
      "source": "/:path*",
      "destination": "/client/dist/index.html"
    }
  ]
}
